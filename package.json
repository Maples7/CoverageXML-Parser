{
  "name": "coveragexml-parser",
  "displayName": "CoverageXML-Parser",
  "description": "A VSCode extension that parses CoverageXML files generated by VS and shows the results inside VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "license": "MIT",
  "categories": [
    "Other",
    "Visualization"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Maples7/CoverageXML-Parser"
  },
  "activationEvents": [],
  "contributes": {},
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "pretty": "eslint --fix --ext .ts",
    "test": "node ./out/test/runTest.js",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "mocha": "^10.0.0",
    "typescript": "^4.8.3"
  }
}
